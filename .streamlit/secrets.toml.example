# Copy this file to secrets.toml and fill in your values
# For Streamlit Cloud, add these in the app settings under "Secrets"
# For EC2/Docker, use environment variables instead

[aws]
region = "us-west-1"
access_key_id = "your-access-key-id"
secret_access_key = "your-secret-access-key"
table_name = "PhotoStorageApp"
s3_bucket = "photobooth-images"  # Optional - defaults to {table_name}-images
